{
    "name": "bog/payment",
    "description": "Laravel package for BOG (Bank of Georgia) payment gateway integration with card management and payment tracking",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "bog",
        "payment",
        "gateway",
        "bank-of-georgia",
        "payment-processing",
        "payment-integration"
    ],
    "version": "1.0.0",
    "authors": [{
        "name": "Your Name",
        "email": "your.email@example.com"
    }],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bog\\Payment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bog\\Payment\\BogPaymentServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
